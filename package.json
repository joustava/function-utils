{
	"name": "@ultraq/function-utils",
	"version": "0.4.0",
	"description": "A collection of utilities for JavaScript functions",
	"author": "Emanuel Rabina <emanuelrabina@gmail.com> (http://www.ultraq.net.nz/)",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ultraq/function-utils.git"
	},
	"keywords": [
		"function",
		"utilities",
		"memoize"
	],
	"module": "function-utils.es.js",
	"main": "function-utils.cjs.js",
	"types": "function-utils.d.ts",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint \"*.js\"",
		"test": "jest",
		"build": "npm run build:cjs && npm run build:es && npm run build:dts",
		"build:cjs": "BABEL_ENV=cjs babel function-utils.js --out-file function-utils.cjs.js --source-maps",
		"build:es": "BABEL_ENV=es babel function-utils.js --out-file function-utils.es.js --source-maps",
		"build:dts": "tsc --allowJs --declaration --emitDeclarationOnly function-utils.js",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.22.15",
		"@babel/plugin-transform-modules-commonjs": "^7.23.3",
		"@types/jest": "^29.5.4",
		"date-fns": "^3.0.6",
		"eslint": "^8.48.0",
		"eslint-config-ultraq": "^3.1.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsdoc": "^46.9.1",
		"jest": "^29.6.4",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=18"
	}
}
